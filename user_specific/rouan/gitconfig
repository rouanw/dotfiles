[user]
  name = Rouan Wilsenach
  email = Rouan.Wilsenach@tesglobal.com
[color]
  diff = auto
  status = auto
  branch = auto
  interactive = auto
  ui = auto
[diff]
  renames = true
[alias]
  changes=diff --name-status -r
  st=status
  co=checkout
  ci=commit
  undo=checkout -f
  pr=pull --rebase
  l=log
  s=status
  a=add -A
  d=diff
  dc=diff --cached
  lg=log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit
  ls=log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit --date=relative -20
  amend=commit --amend --no-edit
  clean-branches="!git branch --merged | egrep -v \"(^\\*|master)\" | xargs git branch -d && git remote prune origin"
[core]
  excludesfile = ~/.gitignore
[mirror]
  summary = true
[branch]
  autosetupmerge = true
[help]
  autocorrect = 1
[push]
	default = current
